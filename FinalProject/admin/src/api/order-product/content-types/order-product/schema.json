      "target": "api::product.product",
      "mappedBy": "order_product"
    },
    "product_sizes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-size.product-size",
      "mappedBy": "order_product"
    },
    "product_colors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-color.product-color",
      "mappedBy": "order_product"
    }
  }
